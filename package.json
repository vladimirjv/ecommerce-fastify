{
  "name": "ecommerce-fastify",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "json2ts": "json2ts -i ./schemas --output ./src/schemas/types --cwd ./schemas/ --$refOptions.dereference.circular=true",
    "dev": "concurrently \"npm:dev:*\" ",
    "dev:server": "nodemon --config nodemon.json src/index.ts",
    "dev:json2ts": "nodemon --watch './schemas/**/*.json' --exec npm run json2ts",
    "debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "prebuild": "npm run json2ts",
    "build": "tsc -p tsconfig.json",
    "start": "node server.js"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "@types/json-schema": "^7.0.10",
    "ajv": "^8.10.0",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-auth0-verify": "^0.7.0",
    "fastify-plugin": "^3.0.1",
    "json-schema-to-typescript": "^10.1.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "json-schema-to-ts": "^1.6.5",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.4",
    "prisma": "^3.11.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
