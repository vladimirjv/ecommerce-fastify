{
  "name": "ecommerce-fastify",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "json2ts": "json2ts -i ./schemas --output ./src/schemas/types --cwd ./schemas/ --$refOptions.dereference.circular=true",
    "dev": "concurrently \"npm:dev:*\" ",
    "dev:server": "nodemon --config nodemon.json src/index.ts",
    "dev:json2ts": "nodemon --watch './schemas/**/*.json' --exec npm run json2ts",
    "debug": "nodemon --config nodemon.json --inspect-brk src/index.ts",
    "prebuild": "npm run json2ts",
    "build": "tsc -p tsconfig.json",
    "start": "node server.js"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "ajv": "^8.10.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-auth": "^1.1.0",
    "fastify-jwt": "^4.1.3",
    "fastify-sensible": "^3.1.2",
    "fastify-swagger": "^5.1.0",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "type-fest": "^2.12.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/http-errors": "^1.8.2",
    "@types/json-schema": "^7.0.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "json-schema-to-typescript": "^10.1.5",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.5.4",
    "prisma": "^3.11.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
